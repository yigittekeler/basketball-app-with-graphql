type Query {
    players(page: Int, size: Int): [Player]
}

type Mutation {
    addPlayer(playerRequest : PlayerRequest): Player
    deletePlayer(id : ID! ) : Player
}
#type CustomResponseEntity {
#    responseEntity: String!
#    statusCode: Int!
#}
input PlayerRequest {
    name: String!
    surname: String!
    position: Position!
}
type Player {
    id: ID!
    name: String!
    surname: String!
    position: String!
}


enum Position {
    PG
    SG
    SF
    PF
    C
}